<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="Cpu Frequency">
        <ListPreference
            android:key="cpu_min_freq_pref"
            android:persistent="true"
            android:summary="Change Cpu Minimum Frequency"
            android:title="Minimum Frequency" />
        <ListPreference
            android:key="cpu_max_freq_pref"
            android:persistent="true"
            android:summary="Change Cpu Maximum Frequency"
            android:title="Maximum Frequency" />

    </PreferenceCategory>

    <PreferenceCategory android:title="Cpu Governor">
        <ListPreference
            android:key="governor_pref"
            android:persistent="true"
            android:summary="Change Scaling Governor"
            android:title="Governors" />

    </PreferenceCategory>
</PreferenceScreen>